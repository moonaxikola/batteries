{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/ui/web/src",
  "projectType": "library",
  "tags": ["web ui"],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/ui/web",
        "tsConfig": "libs/ui/web/tsconfig.lib.json",
        "project": "libs/ui/web/package.json",
        "entryFile": "libs/ui/web/src/index.ts",
        "external": ["@emotion/react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/ui/web/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/ui/web/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui/web"],
      "options": {
        "jestConfig": "libs/ui/web/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
